{
  "name": "fullstack-vue-graphql-starter",
  "version": "1.0.0",
  "description": "Starter for Full-Stack Vue-GraphQL-Apollo Projects",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --exec babel-node --presets env,stage-2  server.js",
    "debug": "node --exec babel-node --presets env,stage-2 --nolazy --inspect-brk=9229 server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "keywords": [],
  "author": "Gerald Michelant",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.0.0-rc.7",
    "apollo-server-express": "^2.2.5",
    "babel-plugin-dotenv": "^0.1.1",
    "bcrypt": "^3.0.0",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "md5": "^2.2.1",
    "neo4j-driver": "^1.7.2",
    "neo4j-graphql-js": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.6.0",
    "nodemon": "^1.18.1"
  },
  "optionalDependencies": {}
}
